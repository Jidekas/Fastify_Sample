{
  "name": "Fastify-api-class",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "dev": "tsnd --respawn --transpile-only --exit-child src/app.ts",
    "update-schema": "node ./scripts/updatePrismaShema.js",
    "migrate": "npx prisma migrate dev --name add_audit_fields",
    "test-migrate": "dotenv -e .env.test -- npx prisma migrate dev --name add_audit_fields",
    "generate": "npx prisma generate",
    "test": "dotenv -e .env.test -- vitest run",
    "test:watch": "vitest",
    "coverage": "vitest run --coverage",
    "docker:up": "docker compose up -d",
    "docker:down": "docker compose down",
    "docker-test": "yarn docker:up && yarn prisma migrate deploy && jest -i"
  },
  "dependencies": {
    "@faker-js/faker": "^8.4.1",
    "@fastify/jwt": "5.0.0",
    "@fastify/swagger": "^8.14.0",
    "@fastify/swagger-ui": "^4.0.0",
    "@prisma/client": "^5.12.1",
    "dotenv": "^16.4.5",
    "dotenv-cli": "^7.4.2",
    "fastify": "^4.26.2",
    "fastify-swagger": "^5.2.0",
    "fastify-zod": "^1.4.0",
    "sinon": "^18.0.0",
    "ts-mock-imports": "^1.3.16",
    "zod": "^3.23.3",
    "zod-to-json-schema": "^3.23.0"
  },
  "devDependencies": {
    "@types/fastify-jwt": "^0.8.1",
    "@types/jest": "^29.5.12",
    "@types/node": "^20.12.7",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.4.5",
    "vite-tsconfig-paths": "^4.3.2",
    "vitest": "^1.6.0",
    "vitest-mock-extended": "^1.3.1"
  }
}
